record(ai, "$(P)A:TEMP") {
    field(DESC, "Temperature Sensor A")
    field(DTYP, "stream")
    field(INP, "@Lakeshore340.proto getTempA $(PORT)")
    field(SCAN, "1 second")
    field(PREC, "5")
    field(EGU, "K")
    
    field(SIML, "$(P)SIM")
    field(SIOL, "$(P)SIM:A:TEMP")
    field(SDIS, "$(P)DISABLE")
}

record(ai, "$(P)A:RDG") {
    field(DESC, "Measurement Sensor A")
    field(DTYP, "stream")
    field(INP, "@Lakeshore340.proto getRdgA $(PORT)")
    field(SCAN, "1 second")
    field(PREC, "5")
    field(EGU, "")
    
    field(SIML, "$(P)SIM")
    field(SIOL, "$(P)SIM:A:RDG")
    field(SDIS, "$(P)DISABLE")
}

record(ao, "$(P)SIM:A:RDG"){}
record(ao, "$(P)SIM:A:TEMP"){}
